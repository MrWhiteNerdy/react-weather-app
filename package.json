{
  "name": "react-weather-app",
  "version": "1.0.0",
  "description": "Shows weather-related info using OpenWeatherMap API, built with React & Redux",
  "main": "index.js",
  "repository": "git@github.com:MrWhiteNerdy/react-weather-app.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Joseph Davis <joseph@josephndavis.com> (https://www.josephndavis.com)",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "openweathermap",
    "weather"
  ],
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^3.1.11"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "babel-preset-stage-1": "^6.1.18",
    "lodash": "^3.10.1",
    "react": "16.3.2",
    "react-dom": "16.3.3",
    "react-redux": "5.0.7",
    "react-sparklines": "^1.7.0",
    "redux": "4.0.0",
    "redux-promise": "^0.6.0"
  }
